{
	"api": {
		"beta": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"securityType": "API_KEY"
			}
		},
		"betaapi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "usersfunction",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"function": {
		"usersfunction": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "api",
					"resourceName": "beta",
					"attributes": [
						"GraphQLAPIIdOutput",
						"GraphQLAPIEndpointOutput"
					]
				}
			]
		}
	},
	"storage": {
		"betaStorage": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	}
}
