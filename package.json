  "private": true,
  "proxy": {
    "/api/*": {
      "target": "http://localhost:5000"
    }
  },
  "dependencies": {
    "aws-amplify": "^1.1.32",
    "aws-amplify-react": "^2.3.11",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.2",
    "date-fns": "^2.0.1",
    "eslint": "^5.16.0",
    "moment": "^2.24.0",
    "nodemon": "^1.19.1",
    "react": "^16.9.0",
    "react-avatar-editor": "^11.0.7",
    "react-color": "^2.17.3",
    "react-dom": "^16.9.0",
    "react-quill": "^1.3.3",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^1.1.5",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.0",
    "uuid": "^3.3.3"
  },
  "scripts": {
